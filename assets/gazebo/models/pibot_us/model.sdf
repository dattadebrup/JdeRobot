<model name="pibot">
    <link name="chasis">
    	<pose>0 0 0 0 0 0</pose>
    	<inertial>
          	<mass>10</mass>
          <inertia>
            <ixx>1</ixx>
          	<ixy>0</ixy>
          	<iyy>1</iyy>
          	<ixz>0</ixz>
          	<iyz>0</iyz>
          	<izz>1</izz>
          </inertia>
      	</inertial>
      	<collision name="collision">
          <geometry>
            <mesh>
              <uri>model://pibot_us/meshes/chasis.dae</uri>
          	</mesh>
          </geometry>
		
       	</collision>
      	<visual name="visual">
          <geometry>
            <mesh>
              <uri>model://pibot_us/meshes/chasis.dae</uri>
            </mesh>
          </geometry>
      	</visual>
    </link>

    <link name="us">
    	<pose>0 0 0 0 0 0</pose>
    	<inertial>
          	<mass>0.05</mass>
          <inertia>
            <ixx>1</ixx>
          	<ixy>0</ixy>
          	<iyy>1</iyy>
          	<ixz>0</ixz>
          	<iyz>0</iyz>
          	<izz>1</izz>
          </inertia>
      	</inertial>
      	<collision name="collision">
          <geometry>
            <mesh>
              <uri>model://pibot_us/meshes/us_sensor.dae</uri>
          	</mesh>
          </geometry>
		
       	</collision>
      	<visual name="visual">
          <geometry>
            <mesh>
              <uri>model://pibot_us/meshes/us_sensor.dae</uri>
            </mesh>
          </geometry>
      	</visual>
	<sensor name="sonar" type="sonar">
          <pose>0.083 -0.004 0.058 0 -1.5707 0</pose>
          <sonar>
            <min>0.03</min>
            <max>4.00</max>
            <radius>0.01</radius>
          </sonar>
          <always_on>1</always_on>
          <update_rate>30</update_rate>
          <visualize>true</visualize>
          <plugin name='camera_dump' filename='libsonarMBOT.so'/>
        </sensor>
    </link>

